@model Min_FrontEndApp.Models.ViewModels.ProductViewModel


<section class="collection-grid">
    <div class="container">
        <div class="title">All products </div>
        <div class="item-collection">
        @foreach (var product in @Model.AllProducts)
        {
            <partial name="_CollectionItemPartial" model="product"/>
        }
        </div>
    </div>
</section>